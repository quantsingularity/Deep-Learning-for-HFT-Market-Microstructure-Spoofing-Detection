# Environment Configuration Example
# Copy this file to .env and update with your values

# Database Configuration
POSTGRES_PASSWORD=change_this_secure_password
POSTGRES_USER=tengnn
POSTGRES_DB=spoofing_detection

# Grafana
GRAFANA_PASSWORD=change_this_admin_password

# Model Configuration
MODEL_PATH=/app/pretrained_models/ten_model_synthetic.pth
CONFIG_PATH=/app/configs/config.json
DEVICE=cuda

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=kafka:9092
KAFKA_TOPIC=lob_events

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# API Configuration
API_URL=http://ten-gnn-cpu:8000
API_KEY=your_api_key_here

# Deployment Configuration
LOG_LEVEL=INFO
WORKERS=4
